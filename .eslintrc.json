{
    "env": {
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "class-methods-use-this": "off",
        "comma-dangle": "off",
        "no-underscore-dangle": "off",
        "jest/expect-expect": "off",
        "no-param-reassign": "off",
        "no-unsafe-optional-chaining": "off",
        "no-shadow": "off",
        "dot-notation": "off",
        "no-console": "error",
        "no-tabs": "off",
        "no-plusplus": "off"
    }
}